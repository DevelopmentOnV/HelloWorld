

@{
    ViewBag.Title = "Login";
}
<style>

   .form-control{
       margin:8px;
   }
</style>
<h2>Sign_UP</h2>

<form>
    <input type="text" class="form-control" id="firstid" placeholder="First_Name" />
    <input type="text" class="form-control" placeholder="Last_Name" />
    <input type="email" class="form-control" placeholder="xyz@gmail.com" />
    <input type="date" class="form-control" placeholder="DOB"/>
    <input type="tel" class="form-control" max="10" placeholder="Mobile_number" />
    <input type="text" class="form-control" placeholder="Age" />
    <input type="text" class="form-control" placeholder="City" />
</form>


<button type="button" class="btn btn-success btn-lg" id="submit">SIGN_UP</button>
<button type="button" class="btn btn-warning btn-lg">RESET</button>


<script src="~/MYJS/ValidationJS.js"></script>
